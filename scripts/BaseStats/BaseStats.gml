///@function (Max Strength, Max Arcane, Max Agility)
///@description Initializes and sets base stats.
///@param Max_Strength
///@param Max_Arcane
///@param Max_Agility
function BaseStats(argument0, argument1, argument2) {

	MAX_STRENGTH = argument0;
	MAX_ARCANE = argument1;
	MAX_AGILITY = argument2;

	STRENGTH = MAX_STRENGTH;
	ARCANE = MAX_ARCANE;
	AGILITY = MAX_AGILITY;



}
